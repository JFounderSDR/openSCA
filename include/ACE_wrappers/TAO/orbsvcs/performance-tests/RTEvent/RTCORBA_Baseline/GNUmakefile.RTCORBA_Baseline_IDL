ifeq ($(rt_corba),1)
ifeq ($(corba_messaging),1)
ifneq ($(ace_for_tao),1)
  ifeq (RTEvent, $(findstring RTEvent, $(TAO_ORBSVCS)))
  ifeq (Naming, $(findstring Naming, $(TAO_ORBSVCS)))
  ifeq (RTCORBAEvent, $(findstring RTCORBAEvent, $(TAO_ORBSVCS)))
  else
    LIB_CHECKED   =
    SHLIB =
  endif
  else
