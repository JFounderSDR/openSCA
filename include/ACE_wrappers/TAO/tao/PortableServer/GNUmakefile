MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: TAO_PortableServer_subset PerlACE-target

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TAO_PortableServer_subset $(@)
	$(KEEP_GOING)@cd PerlACE && $(MAKE) -f GNUmakefile.PerlACE $(@)

.PHONY: TAO_PortableServer_subset
TAO_PortableServer_subset:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TAO_PortableServer_subset all

.PHONY: PerlACE-target
PerlACE-target:
	$(KEEP_GOING)@cd PerlACE && $(MAKE) -f GNUmakefile.PerlACE all

project_name_list:
	@echo TAO_PortableServer_subset
	@echo PerlACE-target
