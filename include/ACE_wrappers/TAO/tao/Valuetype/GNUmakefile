MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: Valuetype_Idl Valuetype

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Valuetype_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Valuetype $(@)

.PHONY: Valuetype_Idl
Valuetype_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Valuetype_Idl all

.PHONY: Valuetype
Valuetype: Valuetype_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Valuetype all

project_name_list:
	@echo Valuetype
	@echo Valuetype_Idl
