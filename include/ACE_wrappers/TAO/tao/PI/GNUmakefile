MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: PI_Idl PI

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.PI_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.PI $(@)

.PHONY: PI_Idl
PI_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.PI_Idl all

.PHONY: PI
PI: PI_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.PI all

project_name_list:
	@echo PI
	@echo PI_Idl
