MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: Messaging_Idl Messaging

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Messaging_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Messaging $(@)

.PHONY: Messaging_Idl
Messaging_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Messaging_Idl all

.PHONY: Messaging
Messaging: Messaging_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Messaging all

project_name_list:
	@echo Messaging
	@echo Messaging_Idl
