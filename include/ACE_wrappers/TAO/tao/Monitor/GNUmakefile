MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: Monitor_Idl TAO_Monitor

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Monitor_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TAO_Monitor $(@)

.PHONY: Monitor_Idl
Monitor_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Monitor_Idl all

.PHONY: TAO_Monitor
TAO_Monitor: Monitor_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TAO_Monitor all

project_name_list:
	@echo Monitor_Idl
	@echo TAO_Monitor
