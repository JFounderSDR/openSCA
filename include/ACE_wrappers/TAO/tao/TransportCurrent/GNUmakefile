MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: TC_Idl TC TC_IIOP_Idl TC_IIOP

REMAINING_TARGETS := $(filter-out all,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC_IIOP_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC_IIOP $(@)

.PHONY: TC_Idl
TC_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC_Idl all

.PHONY: TC
TC: TC_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC all

.PHONY: TC_IIOP_Idl
TC_IIOP_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC_IIOP_Idl all

.PHONY: TC_IIOP
TC_IIOP: TC TC_IIOP_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.TC_IIOP all

