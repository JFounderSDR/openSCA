PI_Server_Idl:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.PI_Server_Idl all

.PHONY: PI_Server
PI_Server: PI_Server_Idl
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.PI_Server all

